{
  "kind": "collectionType",
  "collectionName": "order_items",
  "info": {
    "singularName": "order-item",
    "pluralName": "order-items",
    "displayName": "OrderItem",
    "description": "订单明细"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "items"
    },
    "productId": {
      "type": "integer",
      "required": true
    },
    "productName": {
      "type": "string",
      "required": true
    },
    "productSku": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "subtotal": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "variant": {
      "type": "json"
    }
  }
}
